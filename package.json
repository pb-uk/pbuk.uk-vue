{
  "name": "pbuk-vue",
  "version": "0.1.0",
  "description": "Vue component bundle for pbuk.uk website.",
  "keywords": [
    "boilerplate"
  ],
  "homepage": "https://github.com/pb-uk/pbuk.uk-vue",
  "bugs": "https://github.com/pb-uk/pbuk.uk-vue/issues",
  "repository": {
    "type": "git",
    "url": "git@github.com:pb-uk/pbuk.uk-vue.git"
  },
  "author": "pbuk (http://github.com/pb-uk)",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "browser": "dist/pbuk-vue.min.js",
  "scripts": {
    "build": "npm run lint:fix && npm run build:build",
    "build:build": "rollup -c",
    "ci": "npm run lint && npm run build:build && npm run test:ci",
    "lint": "eslint . && prettier . --check",
    "lint:fix": "eslint . --fix && prettier . --write",
    "test": "jest test --verbose",
    "test:ci": "npm run test",
    "test:coverage": "jest test/unit --coverage",
    "test:func": "jest test/functional --verbose",
    "test:unit": "jest test/unit --verbose"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@primer/css": "^18.0.2-rc.d0feeb3c",
    "@primer/primitives": "^5.1.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^25.0.5",
    "jest": "^27.2.0",
    "prettier": "^2.4.0",
    "rollup": "^2.56.3",
    "rollup-plugin-sass": "^1.2.9",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "rollup-plugin-vue": "^6.0.0",
    "vue": "^3.2.20"
  }
}
